-- Scripts:

local function HPUI_fake_script() -- SnakeHubNoKey.DraggableLocalScript 
	local script = Instance.new('LocalScript', SnakeHubNoKey)

	--By DriedUpCat
	
	frame = script.Parent.MainHub
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(HPUI_fake_script)()
local function RPFUR_fake_script() -- CloseButton.LocalScript 
	local script = Instance.new('LocalScript', CloseButton)

	--Close Button:
	local frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
end
coroutine.wrap(RPFUR_fake_script)()
local function KNMYS_fake_script() -- OpenButton.LocalScript 
	local script = Instance.new('LocalScript', OpenButton)

	--Open Button:
	local frame = script.Parent.Parent.MainHub --Change "Frame" to your frame.
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = true
	end)
end
coroutine.wrap(KNMYS_fake_script)()
